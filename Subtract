#include <iostream>
#include <conio.h>
using namespace std;
 int sub(int,int);
int main()
{
    int num1,num2;
    cout<<"Enter two numbers: \n";
    cin>>num1>>num2;//
     cout<<"Subtraction of two numbers : "<<sub(num1,num2);
     getch();
    return 0;
}
int sub(int num1, int num2)//function definition
{
    if(num2==0)
        return num1;
    else
        return sub((num1-1),(num2-1));
}
import queue
q = queue.LifoQueue()
#insert items at the head of the queue 
for x in range(4):
    q.put(str(x))
#remove items from the head of the queue 
while not q.empty():
    print(q.get(), end=" ")
print()


